#include <stdio.h>

int main()
{
    int choice;
    scanf("%d",&choice);
    switch(choice)
    {
        case1: printf("sunday");
        break;
        case2: printf("monday");
        break;
        case3: printf("tuesday");
        break;
        case4: printf("wednesday");
        break;
        case5: printf("thursday");
        break;
    return 0;
}
