#include <stdio.h>

int main()
{
    int num;
    printf("Enter number: ");
    scanf("%d", &num);
     if(num % 2 == 0) 
    {
        printf("The given number is even.\n");
    } else {
        printf("The given number is odd.\n");
    }
    
    return 0;
}

output:
      Enter number: 6
The given number is even.
    
